{
    "presets": ["next/babel"],
    "plugins": [
      [
        "module-resolver",
        {
          "root": ["./src/"],
          "alias": {
            "@components": "./src/components",
            "@utils": "./src/utils",
            "@api": "./src/api",
            "@services": "./src/services",
            "@globals": "./src/globals",
            "@assets": "./src/assets",
            "@types": "./src/types"
          }
        }
      ]
    ]
  }

  