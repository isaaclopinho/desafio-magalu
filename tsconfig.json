{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "baseUrl": "./src/",
    "paths": {
      "@components/*": [
        "./src/components/*"
      ],
      "@utils/*": [
        "./src/utils/*"
      ],
      "@services/*": [
        "./src/services/*"
      ],
      "@api/*": [
        "./src/api/*"
      ],
      "@globals/*": [
        "./src/globals/*"
      ],
      "@assets/*": [
        "./src/assets/*"
      ],
      "@types/*": [
        "./src/types/*"
      ],
      "@context/*": [
        "./src/context/*"
      ],
  },
  },
  "include": [
    "**/*.ts",
    "**/*.tsx"
, "src/scripts/create-component/index.js", "src/scripts/create-component/index.js"  ]
}
